---
layout: nil
---

<?xml version="1.0"?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
  <channel>
    <title>蚂蚁的网络日志</title>
    <link>http://blog.ownlinux.net</link>
    <atom:link href="http://blog.ownlinux.net/atom.xml" rel="self" type="application/rss+xml" />
    <description></description>
    <language>zh-cn</language>
    <pubDate>{{ site.time | date: "%a, %d %b %Y %H:%M:%S %z" }}</pubDate>
    <lastBuildDate>{{ site.time | date: "%a, %d %b %Y %H:%M:%S %z" }}</lastBuildDate>

    {% for post in site.posts %}i
    {% if post.url contains '2012' %}
    <item>
      <title>{{ post.title }}</title>
      <link>http://example.com{{ post.url }}</link>
      <pubDate>{{ post.date | date: "%a, %d %b %Y %H:%M:%S %z" }}</pubDate>
      <author>nourlcn@gmail.com</author>
      <guid>http://blog.ownlinux.net{{ post.id }}</guid>
      <description>{{ post.content | xml_escape }}</description>
    </item>
    {% endif %}
    {% endfor %}

  </channel> 
</rss>
